{
  "extends": ["html-validate:recommended"],
  "rules": {
    "void-style": "off",
    "close-order": "error",
    "no-dup-id": "error",
    "no-dup-attr": "error",
    "require-sri": "off",
    "script-type": "off",
    "no-inline-style": "off",
    "no-trailing-whitespace": "off",
    "element-permitted-content": "error",
    "element-required-attributes": "error"
  },
  "elements": [
    "html5"
  ]
}
